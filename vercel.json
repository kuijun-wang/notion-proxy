{
  "version": 2,
  "builds": [
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "https://www.notion.so/$1",
      "headers": {
        "Host": "www.notion.so",
        "Referer": "https://www.notion.so/",
        "Origin": "https://www.notion.so"
      }
    }
  ]
}
